{
  "name": "crudgenerator",
  "version": "0.0.1",
  "description": "generates mysql+php CRUDs because my teacher thinks it's ok for us to do the same thing over and over again",
  "main": ".//index.js",
  "bin": {
    "crudgenerator": "./dist/index.js"
  },
  "scripts": {
    "start": "node -r ts-node/register src/index.ts",
    "build": "tsc -p .",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dumbasPL/CRUDGenerator.git"
  },
  "keywords": [
    "crud",
    "mysql",
    "php",
    "generator"
  ],
