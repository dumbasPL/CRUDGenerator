  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/dumbasPL/CRUDGenerator/issues"
  },
  "homepage": "https://github.com/dumbasPL/CRUDGenerator#readme",
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "commander": "^6.2.0"
  }
}
